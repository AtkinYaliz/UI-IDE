{
	"private": true,
	"name": "react-beautify",
	"displayName": "react-beautify",
	"description": "Beautify JS/JSX/TS/TSX Code",
	"version": "0.3.0",
	"publisher": "taichi",
	"galleryBanner": {
		"color": "#61dafb",
		"theme": "light"
	},
	"engines": {
		"vscode": "^1.6.0"
	},
	"categories": [
		"Other",
		"Languages"
	],
	"license": "MIT",
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onCommand:react.beautify"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"title": "react-beautify configuration",
			"properties": {
				"react.beautify.onSave": {
					"description": "Automatically format files on save.",
					"type": "boolean",
					"default": false
				},
				"react.beautify.formatter": {
					"description": "Formatter implementation",
					"type": "string",
					"enum": [
						"prettydiff",
						"esformatter"
					],
					"default": "prettydiff"
				},
				"react.beautify.configFilePath": {
					"description": "Specifies the workspace relative config filepath",
					"type": "string",
					"default": ".jsbeautifyrc"
				}
			}
		},
		"commands": [
			{
				"command": "react.beautify",
				"title": "Beautify JS/JSX/TS/TSX"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"esformatter": "^0.9.6",
		"esformatter-jsx": "^7.0.1",
		"lodash": "^4.16.0",
		"prettydiff": "^1.16.37",
		"resolve-from": "^2.0.0",
		"strip-json-comments": "^2.0.1"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"@types/lodash": "^4.0.0",
		"@types/strip-json-comments": "^0.0.28",
		"@types/resolve-from": "0.0.18",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/taichi/react-beautify"
	},
	"bugs": {
		"url": "https://github.com/taichi/react-beautify/issues"
	},
	"__metadata": {
		"id": "b91512f4-8383-41f9-b939-1fc6f294d2aa",
		"publisherId": "02bb8154-b691-47d8-a7e3-3804a906f3e9",
		"publisherDisplayName": "taichi"
	}
}